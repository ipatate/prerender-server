{
  "name": "prerender-exemple",
  "version": "1.0.0",
  "description": "Example prerender server for render html from web app. Used for SEO robot.",
  "main": "index.js",
  "author": "Faramaz Patrick",
  "license": "ISC",
  "scripts": {
    "test": "jest",
    "build:clean": "rimraf ./dist",
    "start": "npm run build:clean && npm run build && PORT=8083 node index.js",
    "start:debug": "npm run build:clean && npm run build && PORT=8083 node --inspect-brk index.js",
    "build": "babel src --out-dir dist --source-maps",
    "dev": "nodemon --watch src --exec \"npm run flow && npm run start\"",
    "dev:debug": "nodemon --watch src --exec \"npm run flow && npm run start:debug\"",
    "lint": "eslint src",
    "flow": "flow",
    "format": "prettier --write \"**/*.+(js|jsx||json|yml|yaml|css|scss|ts|tsx|graphql|vue|mjs)\" ",
    "flow:build": "flow-remove-types ./src/ -d ./dist/ --all --pretty",
    "validate": "npm run lint && npm run test && npm run flow",
    "precommit": "lint-staged && npm run test && npm run flow"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.3.2",
    "commander": "^2.19.0",
    "consola": "^2.3.0",
    "execution-time": "^1.3.0",
    "express": "^4.16.4",
    "figures": "^2.0.0",
    "fs-extra": "^7.0.1",
    "helmet": "^3.14.0",
    "is-url": "^1.2.4",
    "node-cache": "^4.2.0",
    "ping": "^0.2.2",
    "prerender": "^5.4.4",
    "prerender-memory-cache": "^1.0.2",
    "prerender-node": "^3.2.1",
    "prerender-redis-cache": "^0.2.0",
    "puppeteer": "^1.10.0",
    "puppeteer-prerender": "^0.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-syntax-flow": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-flowtype": "^3.0.0",
    "eslint-plugin-jest": "^21.26.1",
    "eslint-plugin-promise": "^4.0.1",
    "flow-bin": "^0.84.0",
    "flow-remove-types": "^1.2.3",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "nodemon": "^1.18.5",
    "prettier": "^1.14.3"
  }
}
